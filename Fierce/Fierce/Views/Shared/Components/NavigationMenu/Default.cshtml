@model IEnumerable<string>
<a class="btn" style="font-family: 'Roboto', sans-serif; font-size: 25px;"
   asp-action="List"
   asp-controller="Product"
   asp-route-category="">
    Home
</a>
@foreach (string category in Model)
{
    <a class="btn" style="font-family: 'Roboto', sans-serif; font-size: 25px; "
       asp-action="List"
       asp-controller="Product"
       asp-route-category="@category"
       asp-route-productPage="1">
       @category
        </a>
}
